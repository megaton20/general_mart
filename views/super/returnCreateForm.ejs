

    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Sidebar -->
        <%- include ('../partials/superSidebar') %>
        <!-- End of Sidebar -->

        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                <!-- Topbar -->
                <%- include ('../partials/topbar') %>
                <!-- End of Topbar -->
                <%- include ('../partials/messages') %>

                <!-- Begin Page Content -->
                <div class="container-fluid">

                     <!-- DataTales Example -->
                     <div class="card shadow mb-4">

                        <div class="card-body">
                            <div class="form mt-4 mb-4">
                                <form action="/super/create-store" method="post" class="form">
                                   <div class="row">
                                    <div class="col-4">
                                        <div class="form-group">
                                            <label for="">Branch Name</label>
                                            <input type="text" name="Branch_Name" class="form-control" >
                                        </div>
                                    </div>
    
    
                                    <div class="col-4">
                                        <div class="form-group">
                                            <label for="">Branch State</label>
                                            <select name="Branch_state" id="state" class="form-control" onchange="populateLGAs()">
                                                <option value="">Select State</option>
                                                <% stateData.forEach(state => { %>
                                                    <option value="<%= state.state %>"><%= state.state %></option>
                                                    <% }); %>
                                            </select>
                                        </div>
                                    </div>
    
                                   
                                    <div class="col-4">
                                        <div class="form-group">
                                            <label for="">Local Government</label>
                                            <select name="Branch_lga" id="lga" class="form-control">
                                                <option value="">Select State to choose</option>
    
                                            </select>
                                        </div>
                                    </div>
    
                                    <div class="col-4">
                                        <div class="form-group">
                                            <label for="">Branch Address</label>
                                            <input type="text" name="Branch_address" class="form-control" >
                                        </div>
                                    </div>
    
    
    
                                </div>
                                <div class="mt-4">
                                    <input type="submit" class="btn btn-success" style="width: 100%;" value="Submit">
                                </div>
                                </form>
                            </div>
                        </div>
                    </div>

                </div>
                <!-- /.container-fluid -->

            </div>
            <!-- End of Main Content -->

            <!-- Footer -->
            <%- include ('../partials/footer') %>
            <!-- End of Footer -->

        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>
