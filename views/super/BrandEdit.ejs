

    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Sidebar -->
        <%- include ('../partials/superSidebar') %>
        <!-- End of Sidebar -->

        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                <!-- Topbar -->
                <%- include ('../partials/topbar') %>
                <!-- End of Topbar -->

                <!-- Begin Page Content -->
                <div class="container-fluid">
                    <a class="btn btn-outline-primary mb-1 mr-2" href="/super/all-brands">Brands</a>
                    <!-- DataTales Example -->
                    <div class=" mb-4">
                           <!-- DataTales Example -->
                           <div class="card shadow mb-4">
                            <div class="card-body">
                                <div class="form mt-4 mb-4">
                                    <form action="/super/update-brand/<%= results[0].id%>" method="post" class="form">
                                       <div class="row">
                                     
        
                                        <div class="col-6">
                                            <div class="form-group">
                                                <label for="">Brand Name</label>
                                                <input type="text" name="brandName" class="form-control" value="<%= results[0].Name%>" >
                                            </div>
                                        </div>
        
                                    </div>
                                    <div class="mt-4">
                                        <input type="hidden" name="_method" value="PUT">
                                        <input name="_method" type="submit" value="Update" class="btn btn-primary "/>
                                    </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
                <!-- /.container-fluid -->

            </div>
            <!-- End of Main Content -->

            <!-- Footer -->
            <%- include ('../partials/footer') %>
            <!-- End of Footer -->

        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>
