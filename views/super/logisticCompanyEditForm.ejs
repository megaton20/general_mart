
    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Sidebar -->
        <%- include ('../partials/superSidebar') %>
        <!-- End of Sidebar -->

        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                <!-- Topbar -->
                <%- include ('../partials/topbar') %>
                <!-- End of Topbar -->

                <!-- Begin Page Content -->
                <div class="container-fluid">

                    <!-- Page Heading -->
                    <h1 class="h3 mb-2 text-gray-800">Editing "<%= logisticsResults.name %>" </h1>
                    <a href="/super/create-logistics" class="btn btn-outline-primary mr-2 mb-1"aria-controls="collapseTwo">Add Logistics Company</a>
                    <%- include ('../partials/messages') %>

                    <!-- DataTales Example -->
                    <div class=" mb-4">
                            <div class="form mt-4 mb-4">
                                <form action="/super/edit-logistics-company/<%= logisticsResults.id %>" method="post" class="form">
                                   <div class="row">
                                 

                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label for="">company Name</label>
                                            <input type="text" name="name" class="form-control" value="<%= logisticsResults.name %>">
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label for=""> email</label>
                                            <input type="email" name="email" class="form-control" value="<%= logisticsResults.email %>">
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label for="">Phone</label>
                                            <input type="text" name="phone" class="form-control" value="<%= logisticsResults.phone %>">
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label for="">address</label>
                                            <input type="text" name="address" class="form-control" placeholder="Not above 100%" value="<%= logisticsResults.address %>">
                                        </div>
                                    </div>




                                </div>
                                <div class="mt-4">
                                    <input type="hidden" name="_method" value="PUT">
                                    <input name="_method" type="submit" value="Update" class="btn btn-primary "/>
                                </div>
                                </form>
                            </div>
                    </div>

                </div>
                <!-- /.container-fluid -->

            </div>
            <!-- End of Main Content -->

            <!-- Footer -->
            <%- include ('../partials/footer') %>
            <!-- End of Footer -->

        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>
