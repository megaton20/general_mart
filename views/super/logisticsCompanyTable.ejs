

    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Sidebar -->
        <%- include ('../partials/superSidebar') %>
        <!-- End of Sidebar -->

        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                <!-- Topbar -->
                <%- include ('../partials/topbar') %>
                <!-- End of Topbar -->

                <!-- Begin Page Content -->
                <div class="container-fluid">

                    <!-- Page Heading -->
                    <h1 class="h3 mb-2 text-gray-800">Logistics company Tables </h1>
                    <a href="/super/create-logistics" class="btn btn-outline-primary mr-2 mb-1"aria-controls="collapseTwo">Add Logistics Company</a>

                    <%- include ('../partials/messages') %>

                    <!-- DataTales Example -->
                    <div class=" mb-4">
                        <% if(logisticsResults.length <= 0){ %>

                                <h1>No company Added yet !</h1>  

                            <% } else{ %>  
                                <div class="card-body">
                                    <div class="table-responsive">
                                        <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                                            <thead>
                                                <tr>
                                                  <th>Id</th>
                                                  <th>image</th>
                                                  <th>Company Name</th>
                                                  <th>email</th>
                                                  <th>Phone</th>
                                                  <th>Address</th>
                                                  <th>Actions</th>
                                                </tr>
                                            </thead>
                                        
                                            <tbody>
                                          
        
                                                <% logisticsResults.forEach(function(data) {%>
        
                                                    <tr>
                                                        <td><%= data['id'] %></td>
                                                        <td><%= data['image'] %> </td>
                                                        <td><%= data['name'] %></td>

                                                        <td><%= data['email'] %></td>
                                                        <td><%= data['phone'] %></td>
                                                        <td><%= data['address'] %></td>
                                                        <td>
                                                            <a class="btn btn-secondary" href="/super/edit-logistic-company/<%= data['id'] %>"> edit</a>
                                                            <form action="/super/delete-logistic-company/<%= data['id'] %>" method="POST" style="display: inline;">
                                                                <input type="hidden" name="_method" value="DELETE" />
                                                                <button  class="btn btn-danger">Delete</button>
                                                            </form>
                                                        </td>
                                                   </tr> 
                                                   
                                                       <% }) %>
                                       
                                         
                                           
                                            
        
                                                </tbody>
                                        </table>
                                    </div>
                                </div>
                                <% } %>
                        
                    </div>

                </div>
                <!-- /.container-fluid -->

            </div>
            <!-- End of Main Content -->

            <!-- Footer -->
            <%- include ('../partials/footer') %>
            <!-- End of Footer -->

        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

