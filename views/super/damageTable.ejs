

    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Sidebar -->
        <%- include ('../partials/superSidebar') %>
        <!-- End of Sidebar -->

        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                <!-- Topbar -->
                <%- include ('../partials/topbar') %>
                <!-- End of Topbar -->

                <!-- Begin Page Content -->
                <div class="container-fluid">

                    <!-- DataTales Example -->
                    <div class=" mb-4">
                        <% if(allDamaged.length <= 0){ %>

                                
                                <h1>No Damaged Record Found !</h1>  

                            <% } else{ %>  
                                <div class="card-body">
                                    <div class="table-responsive">
                                        <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                                            <thead>
                                                <tr>
                                                    <th>ID</th>
                                                    <th>Product Name</th>
                                                    <th>CategoryID</th>
                                                    <th>WholesalerID</th>
                                                    <th>Unit Price </th>
        
                                                </tr>
                                            </thead>
                                           
                                            <tbody>
                                          
                                                <% allDamaged.forEach(function(data) {%>
        
                                       
                                         
                                                    <tr>
                                                        <td><%= data['id'] %></td>
                                                        <td><%= data['Product_name'] %> </td>
                                                        <td><%= data['Category_Id'] %></td>
                                                        <td><%= data['Supplier_id'] %></td>
                                                        <td><%= data['Unit_price'] %></td>

                                                  
                                                   </tr> 
                                                   
                                                       <% }) %>
        
                                       

                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                                <% } %>
                       
                    </div>

                </div>
                <!-- /.container-fluid -->

            </div>
            <!-- End of Main Content -->

            <!-- Footer -->
            <%- include ('../partials/footer') %>
            <!-- End of Footer -->

        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>
