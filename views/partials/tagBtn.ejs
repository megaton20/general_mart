


  <div class=" mb-4">
    <p class="d-flex justify-content-center">Tags</p>
      <% allTags.forEach(data => { %>
        <a href="/user/tags/<%= data.id %>/products" class=" tagButton" >
         <div class="category-details">
          <div class="category-img">
            <img src="/images/price-tag.png" alt="">
          </div>
          <div class="category-name">
            <p> <%= data.tag_name %> </p>
          </div>
         </div>
      </a>
     
      <% }); %>
  </div>
