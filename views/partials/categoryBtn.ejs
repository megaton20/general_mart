<% allCategory.forEach(data => { %>
    <a href="/user/category/<%= data.Category_name %>#" class=" categoryButton" data-category="<%= data.Category_name %>" title="<%= data.details %>">
     <div class="category-details">
      <div class="category-img">
        <img src="/images/cat/<%= data.image %>" alt="" class="">
      </div>
      <div class="category-name">
        <p> <%= data.Category_name %> </p>
      </div>
     </div>
  </a>
 
  <% }); %>

  <a href="/user/combo/products" class=" categoryButton" data-category="" title="combo items">
    <div class="category-details">
     <div class="category-img">
       <img src="/images/cat/special-offer.png" alt="special-offer" class="">
     </div>
     <div class="category-name">
       <p> Combo items </p>
     </div>
    </div>
 </a>


<% if (userData?.is_exclusive) { %>
  <a href="/user/exclusive/products" class="categoryButton" data-category="exclusive" title="Exclusive items">
    <div class="category-details">
      <div class="category-img">
        <img src="/images/cat/vip.png" alt="" class="">
      </div>
      <div class="category-name">
        <p>Exclusive items</p>
      </div>
    </div>
  </a>
<% } %>